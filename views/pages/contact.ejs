<!-- Contact us form start -->
<div class="container pb-5" id="contactForm">
<div class="row">
    <div class="col">
    <form action="" >
        <div class="pb-3">
        <label for="name" class="form-label">Name</label>
        <input type="text" class="form-control border-dark" placeholder="Enter Your Name">
        </div>
        <div class="pb-3">
        <label for="email" class="form-label">Email Address</label>
        <input type="email" class="form-control border-dark" placeholder="Enter your Email">
        </div>
        <div class="pb-3">
        <label for="number" class="form-label">Phone Number</label>
        <input type="number" class="form-control border-dark" placeholder="Enter Your Phone Number">
        </div>
        <div class="pb-3">
        <label for="Message" class="form-label">Message</label>
        <textarea type="text" class="form-control pb-5 border-dark" placeholder="Enter Your Message"></textarea>
        </div>
        <div class="d-flex justify-content-center" >
        <div type="submit" class="btn btn-primary mt-5 pl-5 pr-5">Submit</div>
        </div>
    </form>
    </div>
</div>
</div>
<!-- Contact us form end -->